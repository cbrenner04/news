<ul class="list-group">
  {{#each articles}}
    <li class="list-group-item">
      <div class="row">
        <div class="col-sm-9">
          <h4 class="list-group-item-heading"><a href="{{ this.link }}">{{ this.title }}</a></h4>
          <p class="list-group-item-text">{{ this.origin }}</p>
        </div>
        <div class="col-sm-3">
          <button class="btn btn-primary pull-right">Save</button>
        </div>
      </div>
    </li>
  {{/each}}
</ul>
